# ignore node dependency directories
node_modules/

# ignore composer dependency directories
vendor/
.vscode/settings.json

# ignore adjusted copies of hosts_example
ansible/hosts
ansible/hosts_myinstance

# ignore failed playbook runs
/ansible/*.retry

# ignore command output
nohup.out

# files generated by make_dockerfile.sh
/.github/docker-*.sh
/Dockerfile

# files generated by running Cypress
/cypress/videos

# ignore where Ansible puts WordPress
/wordpress

# never commit inventory files with private data
/ansible/*_host
/ansible/deploy-*
/ansible/ansible-all.bash
